    <!-- header end-->
    
            
    
    <!--main body-->
    
    <div class="wrapper style1" style="background-color: black;">
        <div class="row py-5 px-4">
            <div class="col-md-9 mx-auto">
        
                <!-- Profile widget -->
                <div class="bg-white shadow rounded overflow-hidden">
                    <div class="px-4 pt-0 pb-4 cover">
                        <div class="media align-items-end profile-head">
                            
                            <div class="media-body mb-5 text-white">
                                <h2 class="mt-0 mb-0" style="padding-bottom: 1.2rem;"><%=foundUser.username%></h2>
                            </div>
                        </div>
                    </div>
                    <div class="bg-light p-4 d-flex justify-content-end text-center">
                        <ul class="list-inline mb-0" style="padding:5%">
                            <li class="list-inline-item" >
                                <h5 class="font-weight-bold mb-0 d-block">6.9/10</h5><small class="text-muted"> <i class="fas fa-star mr-1"></i>Rating</small>
                            </li>
                            <li class="list-inline-item" >
                                <h5 class="font-weight-bold mb-0 d-block">969</h5><small class="text-muted"> <i class="fas fa-user mr-1"></i>Reviews</small>
                            </li>
                            <li class="list-inline-item" >
                                <h5 class="font-weight-bold mb-0 d-block">6969</h5><small class="text-muted"> <i class="fas fa-coins mr-1"></i>Coins</small>
                            </li>
                        </ul>
                    </div>
                    <div class="px-4 py-3">
                        <h5 class="mb-0">About</h5>
                        <div class="p-4 rounded shadow-sm bg-light">
                            <p class="font-italic mb-0">Lives in Ghaziabad</p>
                            <p class="font-italic mb-0">Photographer</p>
                            <p class="font-italic mb-0">Traveller</p>
                        </div>
                    </div>
                    <div class="px-4 py-3">
                        <h5 class="mb-0">Restuarants added</h5>
                        <div class="p-4 rounded shadow-sm bg-light">
                            <section class="carousel" >
                                <div class="reel" >
                                    <% foundUser.restAddedByMe.slice().reverse().forEach(function(rest){%>
                                    <% if(rest.images.length){ %>
                                    <article class="articleImg" >
                                        <a href="/restaurants/<%=rest.id%>" class="image featured"><img src="<%=rest.images[rest.images.length -1].url%>" alt="" /></a>     
                                        <header>
                                            <h3><a href="/restaurants/<%=rest.id%>"><%=rest.name%></a></h3>
                                        </header>     
                                        <ul class="actions">
                                            <li><a href="/restaurant/<%=rest.id%>/offer" class="button alt">Add Offer</a></li>
                                        </ul>          						
                                    </article>
                                    <%}%>
                                    <%})%>                                    	
                                </div>
                                
                            </section>
                        </div>
                    </div>
                    <div class="px-4 py-3">
                        <h5 class="mb-0">Hotels added</h5>
                        <div class="p-4 rounded shadow-sm bg-light">
                            <section class="carousel" >
                                <div class="reel" >
                                    <% foundUser.hotelAddedByMe.slice().reverse().forEach(function(rest){%>
                                    <% if(rest.images.length){ %>
                                    <article class="articleImg" >
                                        <a href="/hotel/<%=rest.id%>" class="image featured"><img src="<%=rest.images[rest.images.length -1].url%>" alt="" /></a>     
                                        <header>
                                            <h3><a href="/hotel/<%=rest.id%>"><%=rest.name%></a></h3>
                                        </header>     
                                        <ul class="actions">
                                            <li><a href="/hotel/<%=rest.id%>/offer" class="button alt">Add Offer</a></li>
                                        </ul>          						
                                    </article>
                                    <%}%>
                                    <%})%>                                    	
                                </div>
                                
                            </section>
                        </div>
                    </div>

                    <div class="px-4 py-3">
                        <h5 class="mb-0">Destinations added</h5>
                        <div class="p-4 rounded shadow-sm bg-light">
                            <section class="carousel" >
                                <div class="reel" >
                                    <% foundUser.destAddedByMe.slice().reverse().forEach(function(rest){%>
                                    <% if(rest.images.length){ %>
                                    <article class="articleImg" >
                                        <a href="/dest/<%=rest.id%>" class="image featured"><img src="<%=rest.images[rest.images.length -1].url%>" alt="" /></a>     
                                        <header>
                                            <h3><a href="/dest/<%=rest.id%>"><%=rest.name%></a></h3>
                                        </header>     
                                        <ul class="actions">
                                            <li><a href="/dest/<%=rest.id%>/offer" class="button alt">Add Offer</a></li>
                                        </ul>          						
                                    </article>
                                    <%}%>
                                    <%})%>                                    	
                                </div>
                                
                            </section>
                        </div>
                    </div>
                    
                </div>
            </div>
        </div>        
    </div>
    
